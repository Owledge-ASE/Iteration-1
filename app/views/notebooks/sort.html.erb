<div class="row top-header">
      <h1 class="col-10">Notebooks</h1>

      <a href="<%= new_notebook_path %>" class="card-link col-2 btn btn-secondary" id="add-note">
        <button type="button" class="btn btn-secondary" >Add Note</button>
      </a>
</div>
<div class="search bar row">

    <div id="sort-description">
      Please click on a column to sort
    </div>
</div>
<div class="grid-notes" id="note-grid">
  
      <%= render partial: 'parts/grid-header' %>
    <% @notes.each do |note| %>
      <%= render partial: 'parts/grid', locals: { note: note } %>
    <% end %>
</div>
</div>
