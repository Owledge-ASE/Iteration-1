<div class="row top-header">
      <h1 class="col-10">Notebooks</h1>

      <a href="<%= new_notebook_path %>" class="card-link col-2 btn btn-secondary" id="add-note">
        <button type="button" class="btn btn-secondary" >Add Note</button>
      </a>
</div>
<div class="search bar">
  <%= form_tag "/notebooks", method: "GET" do%>
    <%= text_field_tag :search_by_contain, params[:search_by_contain], id:"search-bar" %>
    <%= submit_tag "Search", id:"search-button" %>
  <% end %>
</div>
<div class="row card-container" id="notes">
    <% @notes.each do |note| %>
    <%= render partial: 'parts/card', locals: { note: note } %>
    <% end %>
</div>
</div>
